  "devDependencies": {
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "chai": "^4.2.0",
    "eslint": "^7.7.0",
    "eslint-config-google": "^0.14.0",
    "mocha": "^8.1.1",
    "release-it": "^19.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "buffer-to-uint8array": "^1.1.0",
    "canvas": "^2.6.0",
    "canvas-dither": "^1.0.0",
