{
  "name": "react-native-esc-pos-encoder",
  "version": "1.0.6",
  "description": "Create a set of commands that can be send to any receipt printer that supports ESC/POS for React Native",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint --fix src/index.ts",
    "mocha": "mocha --require ts-node/register test/index.test.ts",
    "test": "npm run lint && npm run mocha",
    "release": "release-it",
    "prepare": "bob build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Elleffe-Tech/react-native-esc-pos-encoder.git"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "pos",
    "escpos",
    "receipt",
    "printer"
  ],
  "author": "Francesco Clementi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Elleffe-Tech/react-native-esc-pos-encoder/issues"
  },
  "homepage": "https://github.com/Elleffe-Tech/react-native-esc-pos-encoder#readme",
  "devDependencies": {
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.9.1",
    "chai": "^4.2.0",
    "mocha": "^8.1.1",
    "ts-node": "^10.9.2",
    "@types/jest": "^26.0.0",
    "@types/react": "^18.2.0",
    "@types/react-native": "0.72.3",
    "commitlint": "^11.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^4.2.5",
    "pod-install": "^0.1.38",
    "prettier": "^2.0.5",
    "react": "18.2.0",
    "react-native": "0.72.5",
    "react-native-builder-bob": "^0.18.0",
    "react-native-vision-camera": "4.0.1",
    "react-native-worklets-core": "^1.1.1",
    "release-it": "^16.1.3",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "buffer-to-uint8array": "^1.1.0",
    "canvas": "^2.6.0",
    "canvas-dither": "^1.0.0",
    "canvas-flatten": "^1.0.0",
    "iconv-lite": "^0.6.2",
    "linewrap": "^0.2.1"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  }
}
